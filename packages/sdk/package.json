{
  "name": "@byok-marketplace/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for BYOK Developer App Marketplace - Build AI apps that run on any provider (cloud APIs + local models)",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.mjs",
      "require": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./chat": {
      "import": "./dist/chat/index.mjs",
      "require": "./dist/chat/index.js",
      "types": "./dist/chat/index.d.ts"
    },
    "./images": {
      "import": "./dist/images/index.mjs",
      "require": "./dist/images/index.js",
      "types": "./dist/images/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "sdk",
    "typescript",
    "byok",
    "developer",
    "marketplace",
    "openai",
    "anthropic",
    "claude",
    "ollama",
    "local-ai",
    "tree-shaking"
  ],
  "author": "BYOK Developer App Marketplace",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.63.0",
    "@anthropic-ai/sdk": "^0.28.0",
    "axios": "^1.7.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^9.18.0",
    "jest": "^30.0.5",
    "ts-jest": "^29.2.5",
    "tsup": "^8.4.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ai-app-marketplace/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ai-app-marketplace/sdk/issues"
  },
  "homepage": "https://github.com/ai-app-marketplace/sdk#readme"
}