{
  "name": "byok-developer-app-marketplace",
  "version": "0.1.0",
  "description": "BYOK Developer App Marketplace - Where developers create AI apps that users run with their choice of cloud APIs or local models",
  "keywords": [
    "ai",
    "marketplace",
    "byok",
    "developer",
    "apps",
    "ollama",
    "local-ai",
    "openai",
    "anthropic"
  ],
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:stable": "./scripts/dev-server-stable.sh",
    "health-check": "./scripts/health-check.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "prisma generate",
    "test-endpoints": "curl -I http://localhost:3000 && curl -I http://localhost:3000/marketplace && curl -I http://localhost:3000/marketplace/apps/pdf-notes-generator",
    "pre-change-check": "npm run type-check && npm run health-check && npm run test-endpoints",
    "post-change-verify": "npm run health-check && npm run test-endpoints && echo 'Post-change verification completed'",
    "emergency-restart": "pkill -f 'next dev' || true && lsof -ti:3000 | xargs kill -9 2>/dev/null || true && echo 'Restarting server...' && npm run dev",
    "server-status": "lsof -i :3000 || echo 'No process on port 3000'",
    "monitor-logs": "tail -f server.log | grep -E '(error|Error|ERROR|warning|Warning|WARN)'",
    "validate-env": "echo 'Checking development environment...' && node -v && npm -v && echo 'Environment validated'",
    "test:migration": "tsx scripts/test-migration.ts",
    "migrate:api-keys": "echo 'API key migration is handled automatically on user login. No manual migration needed.'",
    "notify:task-complete": "source scripts/notifications.sh && notify_task_complete",
    "notify:need-approval": "source scripts/notifications.sh && notify_need_approval",
    "notify:qa-complete": "source scripts/notifications.sh && notify_qa_complete",
    "notify:phase-complete": "source scripts/notifications.sh && notify_phase_complete",
    "notify:error": "source scripts/notifications.sh && notify_error",
    "notify:test": "source scripts/notifications.sh && test_notifications"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.8.0",
    "@google-cloud/kms": "^5.1.0",
    "@google-cloud/secret-manager": "^6.1.0",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/multer": "^2.0.0",
    "@types/pdf-parse": "^1.1.5",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.0",
    "form-data": "^4.0.4",
    "gsap": "^3.12.5",
    "lucide-react": "^0.525.0",
    "multer": "^2.0.2",
    "next": "15.4.3",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "prisma": "^6.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.61.1",
    "recharts": "^3.1.0",
    "sonner": "^2.0.6",
    "stripe": "^18.3.0",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.0.5",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5"
  }
}
